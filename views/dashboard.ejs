  <div class="container">

    <ul class="nav nav-pills mb-3" id="pills-tab" role="tablist">
      <li class="nav-item">
        <a class="nav-link active" id="pills-home-tab" data-toggle="pill" href="#pills-home" role="tab"
          aria-controls="pills-home" aria-selected="true">My Wall</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" id="pills-profile-tab" data-toggle="pill" href="#pills-profile" role="tab"
          aria-controls="pills-profile" aria-selected="false">Add Blog</a>
      </li>
    </ul>
    <!--/.Navbar-->
    <div class="row">

      <!-- Card Wider -->
      <% dashboard.continent.forEach(function (wall) { %>
      <!-- Card -->
      <div class="bl card col-md-4 h-100">

        <!-- Card image -->
        <div class="view overlay">
          <img class="card-img-top" src="https://mdbootstrap.com/img/Mockups/Lightbox/Thumbnail/img%20(67).jpg"
            alt="Card image cap">
          <a href="#!">
            <div class="mask rgba-white-slight"></div>
          </a>
        </div>

        <!-- Card content -->
        <div class="card-body">

          <!-- Title -->
          <h4 class="card-title"><%= wall.blog.title %></h4>
          <!-- Text -->
          <p class="card-text"><%= wall.blog.description %></p>
          <!-- Button -->
          <a href="#" class="btn btn-primary">Read More <i class="fas fa-angle-double-right"></i></a>

        </div>

      </div>
      <!-- Card -->
      <% }) %>
      
    </div>

    <!-- Card Light -->

    <!-- Card Light -->
  </div>

